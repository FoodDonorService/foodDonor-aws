# Terraform Backend Configuration
# Copy this file to backend.tf and update with your actual values
# Or use terraform init -backend-config=backend-config.hcl

terraform {
  backend "s3" {
    bucket         = "food-donor-terraform-state-dev"  # 실제 bucket 이름으로 변경
    key            = "terraform.tfstate"
    region         = "ap-northeast-2"
    encrypt        = true
    dynamodb_table = "food-donor-terraform-locks-dev"  # 실제 table 이름으로 변경
  }
}
