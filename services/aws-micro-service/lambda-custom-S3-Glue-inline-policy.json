{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3WriteAccessRaw",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::<RAW_DATA_BUCKET>/*"
            ]
        },
        {
            "Sid": "GlueJobStartAccess",
            "Effect": "Allow",
            "Action": [
                "glue:StartJobRun"
            ],
            "Resource": [
                "arn:aws:glue:ap-northeast-2:<AWS_ACCOUNT_ID>:job/<GLUE_JOB_NAME>"
            ]
        }
    ]
}