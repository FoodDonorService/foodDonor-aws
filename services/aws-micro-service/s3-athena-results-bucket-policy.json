{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowAthenaServiceAccess",
            "Effect": "Allow",
            "Principal": {
                "Service": "athena.amazonaws.com"
            },
            "Action": [
                "s3:GetBucketLocation",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:ListBucketMultipartUploads",
                "s3:ListMultipartUploadParts",
                "s3:AbortMultipartUpload",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::food-donor-athena-results-v1",
                "arn:aws:s3:::food-donor-athena-results-v1/*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:SourceAccount": "436471025878"
                },
                "ArnLike": {
                    "aws:SourceArn": "arn:aws:athena:ap-northeast-2:436471025878:workgroup/*"
                }
            }
        }
    ]
}
